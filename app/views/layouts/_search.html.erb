<div class="">
  <div>
    <div>
      <%= form_with url: search_path, method: :get, local: true do |f|%>
        <%= f.text_field :value %>
        <%= f.select :model, [["User", "user"], ["Book", "book"]] %>
        <%= f.select :how, [["完全一致", "match"], ["前方一致", "forward"], ["後方一致", "backword"], ["部分一致","partical"]]%>
        <%= f.submit '検索' %>
      <% end %>
    </div>
  </div>
</div>